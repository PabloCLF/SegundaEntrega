<h2>Bienvenido</h2>
{{!-- Cart Id: <input type="text">
<button>Set</button> --}}
<h4>Productos disponibles:</h4>
{{#each products}}
<div>
    <h3>{{title}}</h3>
    <p>{{price}}</p>
    <p>{{description}}</p>
    <img src="{{thumbnail}}">
    <form action="/api/carts/6546d7ea8de78809a99e181e/products/{{id}}" method="post">
        <input onclick="event.preventDefault();" type="submit">
    </form>
</div>
{{/each}}